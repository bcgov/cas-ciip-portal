{
  "name": "ggircs-industry-ciip-portal",
  "version": "2.3.1",
  "description": "The online portal for Industry to apply and track their CIIP applications",
  "scripts": {
    "pretest": "npm run build:relay",
    "test": "jest",
    "pretest:e2e-snapshots": "node cypress/support/listAuthenticatedPages.js",
    "test:e2e-snapshots": "yarn happo-cypress -- -- cypress run",
    "pretest:e2e": "node cypress/support/listAuthenticatedPages.js",
    "test:e2e": "cypress run",
    "test:e2e-with-mocks": "cypress run --config integrationFolder=cypress/integration_mocks",
    "precypress": "node cypress/support/listAuthenticatedPages.js",
    "cypress": "cypress open",
    "cucumber:pretty": "cucumber-js --format node_modules/cucumber-pretty --format-options '{\"descriptionEnabled\":true}'",
    "predev": "npm run build:schema && npm run build:relay",
    "dev": "node server",
    "build": "npm run build:schema && npm run build:relay && npm run build:next",
    "build:schema": "postgraphile -X -c postgres://localhost/ciip_portal_dev -s ggircs_portal --export-schema-graphql server/schema.graphql --export-schema-json server/schema.json --classic-ids --sort-export && echo >> server/schema.json",
    "build:relay": "relay-compiler --verbose",
    "build:next": "next build",
    "start": "NODE_ENV=production node --unhandled-rejections=strict server",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "engines": {
    "node": "12.20.1",
    "yarn": "1.22.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@graphile-contrib/pg-many-to-many": "^1.0.0",
    "@rjsf/core": "^2.5.1",
    "body-parser": "^1.19.0",
    "bowser": "^2.11.0",
    "connect-pg-simple": "^6.2.1",
    "consola": "^2.15.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "decimal.js-light": "^2.5.1",
    "deep-diff": "^1.0.2",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphile-build": "^4.11.0",
    "graphile-build-pg": "^4.11.2",
    "graphile-worker": "^0.8.1",
    "graphql": "^15.5.0",
    "graphql-voyager": "^1.0.0-rc.30",
    "html-template-tag": "^4.0.0",
    "isomorphic-fetch": "^3.0.0",
    "js-file-download": "^0.4.12",
    "json-schema": "^0.2.5",
    "keycloak-connect": "^10.0.0",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "next": "^9.5.5",
    "nodemailer": "^6.4.8",
    "pg": "^8.2.0",
    "postgraphile": "^4.11.0",
    "postgraphile-log-consola": "^1.0.1",
    "postgraphile-plugin-connection-filter": "^2.1.1",
    "prop-types": "^15.5.8",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.3",
    "react-bootstrap-typeahead": "^4.2.3",
    "react-cookie": "^4.0.3",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.12.0",
    "react-mathjax2": "^0.0.2",
    "react-number-format": "^4.3.1",
    "react-relay": "^11.0.1",
    "react-relay-network-modern": "^6.0.0",
    "react-relay-network-modern-ssr": "^1.4.0",
    "react-toastify": "^7.0.3",
    "relay-runtime": "^11.0.1",
    "remove": "^0.1.5",
    "styled-jsx": "^3.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/react-relay": "^11.0.1",
    "@types/relay-runtime": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.15.0",
    "axe-core": "^4.1.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-relay": "^11.0.1",
    "babel-preset-react-app": "^10.0.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "cypress": "^6.9.1",
    "cypress-axe": "^0.12.2",
    "easygraphql-tester": "^6.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-relay": "^1.8.2",
    "graphql-query-test-mock": "^0.12.0",
    "graphql-tag-pluck": "^0.8.5",
    "happo-cypress": "^1.20.0",
    "happo.io": "^6.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-extended": "^0.11.5",
    "jest-mock-random": "^1.0.2",
    "jest-raw-loader": "^1.0.1",
    "json-schema-faker": "^0.5.0-rc23",
    "nock": "^9.6.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.12.0",
    "relay-compiler": "^11.0.1",
    "relay-compiler-language-typescript": "14.0.0",
    "relay-config": "^11.0.1",
    "relay-test-utils": "^11.0.1",
    "require-context.macro": "^1.2.2",
    "standard-version": "^9.3.0",
    "typescript": "4.2.3",
    "wait-on": "^5.3.0",
    "webpack": "^4.43.0"
  },
  "resolutions": {
    "**/node-fetch": "^2.6.1",
    "next/resolve-url-loader/adjust-sourcemap-loader/object-path": "0.11.5",
    "**/hosted-git-info": "4.0.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/cypress/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less)$": "identity-obj-proxy",
      "react-syntax-highlighter/dist/esm/(.*)": "react-syntax-highlighter/dist/cjs/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>tests/setupTests.ts",
      "jest-canvas-mock",
      "jest-extended"
    ],
    "transform": {
      ".*(ts|tsx|js|jsx)$": "babel-jest",
      "\\.graphql$": "jest-raw-loader"
    }
  }
}
