{
  "name": "ggircs-industry-ciip-portal",
  "version": "1.0.0",
  "description": "The online portal for Industry to apply and track their CIIP applications",
  "scripts": {
    "pretest": "npm run relay",
    "test": "jest",
    "predev": "npm run relay",
    "dev": "node server",
    "prebuild": "npm run relay",
    "build": "next build",
    "start": "NODE_ENV=production node server",
    "relay": "relay-compiler --src ./ --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' '**/server/**' --schema ./server/schema.graphql --verbose",
    "format": "xo --fix",
    "lint": "xo"
  },
  "xo": {
    "extends": [
      "xo-react",
      "plugin:jest/recommended",
      "plugin:relay/recommended"
    ],
    "plugins": [
      "jest",
      "relay"
    ],
    "envs": [
      "node",
      "browser",
      "jest"
    ],
    "space": true,
    "prettier": true,
    "parser": "babel-eslint",
    "rules": {
      "unicorn/filename-case": 0,
      "import/no-unassigned-import": 0
    }
  },
  "engines": {
    "node": "10.14.1",
    "yarn": "1.12.3"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "diff": "^4.0.1",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "express-session": "^1.16.2",
    "graphql": "^14.4.2",
    "graphql-relay": "^0.6.0",
    "isomorphic-unfetch": "^3.0.0",
    "jquery": "^3.3.1",
    "keycloak-connect": "^7.0.0",
    "next": "^9.0.0",
    "open": "^6.4.0",
    "pg": "^7.11.0",
    "postgraphile": "^4.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.9.0",
    "react-mathjax": "^1.0.1",
    "react-mathjax2": "^0.0.2",
    "react-relay": "^5.0.0",
    "sinon": "^7.4.1",
    "survey-creator": "^1.1.0",
    "survey-react": "^1.1.1",
    "surveyjs-widgets": "latest"
  },
  "resolutions": {
    "graphql-cli/**/lodash": "^4.17.13",
    "graphql-cli/**/npm-run": "^5.0.1",
    "graphql-cli/**/open": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^9.1.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-relay": "^5.0.0",
    "easygraphql-tester": "^5.1.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-relay": "^1.3.11",
    "graphql-cli": "^3.0.11",
    "graphql-query-test-mock": "^0.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-canvas-mock": "^2.1.1",
    "nock": "^10.0.6",
    "node-fetch": "^2.6.0",
    "relay-compiler": "^6.0.0",
    "relay-test-utils": "^5.0.0",
    "xo": "^0.24.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>lib/setupTests.js",
      "jest-canvas-mock"
    ]
  }
}
