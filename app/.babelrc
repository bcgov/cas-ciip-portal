{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["relay", { "artifactDirectory": "./__generated__" }],
    ["module-resolver", {"root": [".", ".."], "extensions": [".js", ".jsx",".ts", ".tsx"]}],
    "macros",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator"
  ]
}
